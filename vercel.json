{
  "version": 2,
  "builds": [
    {
      "src": "project_name/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "project_name/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SECRET_KEY": "your-secret-key",
    "DJANGO_DEBUG": "False",
    "DJANGO_ALLOWED_HOSTS": "*"
  }
}
