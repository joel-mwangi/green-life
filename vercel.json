{
    "version": 2,
    "builds": [
      {
        "src": "project_name/wsgi.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "project_name/wsgi.py"
      }
    ]
  }
  